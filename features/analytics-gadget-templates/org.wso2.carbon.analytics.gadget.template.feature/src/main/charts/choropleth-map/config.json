        {
            "fieldLabel": "Map type",
            "fieldName": "mapType",
            "fieldType": "dropdown",
            "valueSet": ["world","usa","europe"]
        }
    ],
    "imports": {
        "shared-libs":{
            "js" :["analytics-wso2/d3.min.js", "analytics-wso2/vega.js",  "analytics-wso2/VizGrammar.min.js",
                   "analytics-wso2/wso2gadgets.js", "analytics-wso2/chart-utils.js"]
        }
    }
}


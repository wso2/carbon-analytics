{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "transform-es3-member-expression-literals",
    "transform-es3-property-literals"
  ],
  "env": {
    "test": {
      "plugins": [
        ["istanbul", {
          "exclude": [
            "tests.webpack.js",
            "**/*.test.js"
          ]
        }]
      ]
    }
  }
}
